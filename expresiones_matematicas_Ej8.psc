
/// 8. 260 / 12 + 54 % 3 - 85 % 7
// 260 / 12 = 21.666666666666668
// 54 % 3 = 0
// 85 % 7 = 1
// 21.666666666666668 + 0 - 1 = 20.666666666666668

Algoritmo  Expresiones_matematicas_EJ8
	
    Definir num1, num2, num3, num4, num5 Como Real;
    num1 <- 260;
    num2 <- 12;
    num3 <- 54;
    num4 <- 3;
    num5 <- 85;
    Escribir (num1 / num2) + (num3 % num4) - (num5 % 7);
	
FinAlgoritmo

